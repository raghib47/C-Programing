#include<stdio.h>
int main(){
    int m, n, sum = 0; 
    printf("enter the value of m :");
    scanf("%d", &m);
    printf("enter the value of n :");
    scanf("%d", &n);

    while (m <= n) {
        sum = sum + m;
        m = m + 1;
    }

    printf("sum of numbers from m to n is : %d", sum); 
    return 0; 
}
